.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 88rpx;
}

.title {
  font-size: 36rpx;
  font-weight: 700;
}

.slot:empty + .default {
  display: flex;
}

.default {
  display: none;
  align-items: center;
  font-size: 28rpx;
  color: #777;
}

.icon {
  width: 50rpx;
  height: 50rpx;
}